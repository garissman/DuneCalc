        "@vue/test-utils": "^2.4.6",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-vue": "^9.32.0",
        "jsdom": "^28.1.0",
        "playwright": "^1.58.2",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.11",
